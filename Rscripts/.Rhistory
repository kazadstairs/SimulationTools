MakePlot <- function(string.RM, string.QM)
{
RMsym <- sym(string.RM)
PMsym <- sym(string.QM)
library(ggplot2)
#library(tidyverse)
myDF.plot <- myDF %>%
group_by(Instance.Name,Schedule.Type) %>%
summarize(string.RM = !!RMsym,Cmaxsd = sd(Cmax),Cmax=mean(eval(Cmax)))
p <- ggplot(myDF.plot,aes(x=RM,y=PM,colour=Schedule.Type,shape=Schedule.Type))
p <- p + geom_point()
p <- p + geom_errorbar(aes(ymin=PM-PMsd,ymax=PM+PMsd))
p <- p + scale_x_continuous(expand = c(0, 0),limits = c(0,2000))
p <- p + scale_y_continuous(expand = c(0, 0),limits= c(0,500))
p
}
